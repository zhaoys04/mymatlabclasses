function f = Get_Wavelength(PM)
    if PM.ConnectedByUSB
        wavelength = query(PM.device_obj,'PM:Lambda?');
        PM.Wavelength = str2num(wavelength);
        f = 1;
    else
        wavelength = query(PM.device_obj,'PM:Lambda?');
        PM.Wavelength = str2num(wavelength);
        f = 1;
    end
end
